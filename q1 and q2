#oddmachine
#quest2
def reverse_1(input_string):
    words = input_string.split()
    reversed_words = []
    for word in words:
        reversed_word = word[::-1]
        reversed_words.append(reversed_word)
    result = ' '.join(reversed_words)
    return result
input_string = "ViDeo SurVeiLLance SyStems"
output = reverse_1(input_string)
print(output)

output

