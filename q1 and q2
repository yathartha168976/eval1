import numpy as np
def check_strings(s1, s2):
    s1 = s1.lower()
    s2 = s2.lower()
    common_chars = ''
    uncommon_chars_s1 = ''
    uncommon_chars_s2 = ''
    i=0
    j=0
    while i < len(s1) and j < len(s2):
        if s1[i] == s2[j]:
            common_chars = common_chars+ s1[i]
            i= i+1
            j =j+1
    
    uncommon_chars_s1 = s1[i:]
    uncommon_chars_s2 = s2[j:]

    if common_chars == s1 or common_chars == s2:
        print("Yes")
    else:
        print("No")

    print("String1 (Common Characters):", common_chars)
    print("String2 (Uncommon Characters):", uncommon_chars_s1 + uncommon_chars_s2)

check_strings("Refer", "Rafael")
check_strings("Refer", "Regfree")




